#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
void test()
{
	vector<int>v;
	for (int i = 0; i < 10; i++)
	{
		v.push_back(i);
	}
bool  ret=	binary_search(v.begin(), v.end(), 9);

if (ret)
{
	cout << "you can find this";
}
else
{
	cout << "you can't find this" << endl;
}
}
int main()
{
	test();
	return 0;
}
