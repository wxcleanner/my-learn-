#include<iostream>
#include<vector>
using namespace std;
class greatfive
{
public:
	bool operator()(int val)
	{
		return val > 5;
	}
};
void test()
{
	vector<int>v;
	for (int i = 0; i < 10; i++)
	{
		v.push_back(i);
	}
	find_if(v.begin(), v.end(), greatfive());

	vector<int>::iterator it = find_if(v.begin(), v.end(), greatfive());
	if (it == v.end())
	{
		cout << "æ— " << endl;
	}
	else
	{

		cout << *it << endl;
	}
}
int main()
{
	test();
	return 0;
} 
