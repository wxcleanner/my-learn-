#include<iostream>
using namespace std;
int main()
{
	int n, a, b, c, d, e;
	for (n = 5;; n++)
	{
		b = (n - 1) * 4 / 5;
	 c = (b - 1) * 4 / 5;
	 d = (c - 1) * 4 / 5;
	 e  = (d - 1) * 4 / 5;
		if ( n % 5 == 1 && b % 5 == 1 && c % 5 == 1 && d % 5 == 1&&e%5==1)
		{
			cout << n << endl;
			break;
		}
	}


	return 0;
}
