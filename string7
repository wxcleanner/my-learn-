#include<iostream>
using namespace std;
void test()
{
	string str = "abcdef";
	string str1 = str.substr(1, 3);
	cout << str1 << endl;
	string email = "weixiang@qq.com";
	int pos = email.find('@');
	string username = email.substr(0, pos);
	cout << username << endl;
}
int main()
{
	test();
	return 0;
}
