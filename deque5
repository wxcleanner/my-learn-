#include<iostream>
using namespace std;
#include<deque>

void test()
{
	deque<int>d1;
	d1.push_back(100);
	d1.push_back(1000);
	d1.push_front(100);
	d1.push_front(1000);
	for (int i = 0; i < d1.size(); i++)
	{
		cout << d1[i] << " ";
	}
	cout << endl;
	for (int i = 0; i < d1.size(); i++)
	{
		cout << d1.at(i) << " ";
	}
	cout << endl;
	cout << d1.front();
	cout << d1.back();
}
int  main()
{
	test();

}
