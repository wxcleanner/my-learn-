#include<iostream>
using namespace std;
#include<algorithm>
#include<vector>
void test()
{
	vector<int>v;
	v.push_back(1);
	v.push_back(5);
	v.push_back(3);
	v.push_back(6);
	v.push_back(6);
	v.push_back(7);
	vector<int>::iterator pos=adjacent_find(v.begin(), v.end());
	if (pos == v.end())
	{
		cout << "you cn't find this";
}
	else
	{
		cout << "you acn find this" << endl;
	}
}
int main()
{
	test();
	return 0;
}
