#include<iostream>
using namespace std;
void test()
{
	string str1 = "hello world";
	for (int i = 0; i < str1.size(); i++)
	{
		cout << str1[i] << " ";
	}
	for (int i = 0; i < str1.size(); i++)
	{
		cout << str1.at(i) << " ";
	}
	str1[0] ='x';
	str1[1] = 'x';
	cout << str1 << endl;
}
int main()
{
	test();
	return 0;
}
